datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id        String  @id @default(uuid())
  name      String
  email     String  @unique
  password  String
  is_barber Boolean @default(false)

  @@map("accounts")
}
